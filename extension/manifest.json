{
  "manifest_version"      : 2,
  "name"                  : "CMS",
  "version"               : "0.0.1",
  "background"            : {
    "scripts"             : ["background.js"],
    "chrome_url_overrides": {
      "newtab"            : "newtab.html"
    }
  },
  "permissions"           : [
    "tabs",
    "notifications"
  ],
  "browser_action"        : {
    "default_title"       : "CMS"
  }
}